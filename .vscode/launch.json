    "version": "0.2.0",
    "configurations": [
        {
            "name": "(gdb) zerovox",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/build/src/zerovox",
            "args": [],
            "stopAtEntry": false,
            //"cwd": "${fileDirname}",
            "cwd": "${workspaceFolder}",
            "environment": [],
            "externalConsole": false,
            "MIMode": "gdb",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                },
                {
                    "description": "Set Disassembly Flavor to Intel",
                    "text": "-gdb-set disassembly-flavor intel",
                    "ignoreFailures": true
                }
            ]
        },
        {
            "name": "zv2gguf",
            "type": "debugpy",
            "request": "launch",
            "program": "utils/zv2gguf.py",
            "console": "integratedTerminal",
            "args": [],
            "cwd": "${workspaceFolder}",
            "justMyCode": false
        },
        {
            "name": "ggmltest",
            "type": "debugpy",
            "request": "launch",
            "program": "ggmltest.py",
            "console": "integratedTerminal",
            "args": [],
            "cwd": "${workspaceFolder}",
            "justMyCode": false
        },
        {
            "name": "embtest",
            "type": "debugpy",
            "request": "launch",
            "program": "utils/embtest.py",
            "console": "integratedTerminal",
            "args": [],
            "cwd": "${workspaceFolder}",
            "justMyCode": false
        }
    ]
}